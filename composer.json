{
    "name": "pyncer/core",
    "type": "library",
    "description": "Core functions used by the Pyncer library.",
    "homepage": "https://pyncer.com/core",
    "license": "Unlicense",
    "require": {
        "php": ">=8.3",
        "ext-mbstring": "*",
        "pyncer/exception": "^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^1.9"
    },
    "autoload": {
        "files": ["src/autoload.php"],
        "psr-4": {
            "Pyncer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pyncer\\Tests\\Core\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/pyncerrc/pyncer-core/issues",
        "source": "https://github.com/pyncerrc/pyncer-core"
    },
    "extra" : {
        "branch-alias" : {
            "dev-main" : "1.3-dev"
        }
    }
}
